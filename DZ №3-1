#include <iostream>
#include <cmath>
using namespace std;

double saaaaske(double s) {
	double a = sqrt(4 * s / sqrt(3));
	return a;

}
double narutoooo(double n) {
	double m = sqrt(2 * pow(n, 2));
		return m;
}


int main() {
	double o,g,l;
	cin >> o;
	g= saaaaske(o);
	l = narutoooo(g);
	cout << g << endl;
	cout << l << endl;
	return 0;
}
