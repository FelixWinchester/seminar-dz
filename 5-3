#include <iostream>  
 using namespace std;  
 int main()  
 {  
         int n;  
         cin >> n;  
         int *a = new int[n];  
         for (int i = 0; i < n; i++) {  
                 cin >> a[i];  
         }  
         for (int i = 0; i < n; i++) {  
                 if (a[i] == 0) {  
                         cout << i << " ";  
                 }  
         }  
         return 0;  
 }
